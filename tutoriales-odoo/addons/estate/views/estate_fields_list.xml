<?xml version="1.0"?>

<odoo>
    <data>
        <record id="view_test_tree" model="ir.ui.view">
            <field name="name">Test Tree View</field>
            <field name="model">test_model</field>
            <field name="arch" type="xml">
                <tree string="Tests">
                    <field name="name" />
                    <field name="description" />
                    <field name="postcode"></field>
                    <field name="state"></field>
                    <field name="expected_price"></field>
                    <field name="selling_price"></field>
                    <field name="bedrooms"></field>
                    <field name="living_area"></field>
                    <field name="garden_area"></field>
                    <field name="garden"></field>
                    
                </tree>
            </field>
        </record>
    </data>
</odoo>